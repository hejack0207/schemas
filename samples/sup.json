    "commands": {
        "showTime":{
            "desc": "show current time of host",
            "run": "date"
        },
        "ping":{
            "local": "ping 10.0.50.166"
        },
        "shell": {
            "stdin": true,
            "run": "bash"
        }
    }
    
}