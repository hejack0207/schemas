{
    "$schema": "../json/sup.json",
    "version": 0.4,
    "env": {
        "SUP_USER": "root"
    },
    "networks": {
        "test": {
            "hosts": [
                "root@10.0.52.1"
            ]
        },
        "intgr": {
            "user": "root",
            "hosts": [
                "10.0.50.166",
                "10.0.50.167"
            ]
        }
